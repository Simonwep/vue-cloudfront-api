{
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
        "_storagePath": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-cond-assign": "off",
        "no-unused-vars": "warn"
    }
}

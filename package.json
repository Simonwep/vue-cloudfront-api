{
    "name": "vue-cloudfront-api",
    "version": "0.0.1",
    "description": "Official RESTful api for vue-cloudfront",
    "author": "Simon Reinisch",
    "license": "MIT",
    "keywords": [],
    "main": "/src/app.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vue-cloudfront/vue-cloudfront-api"
    },
    "scripts": {
        "dev": "nodemon ./src/app.js --ignore '/_storage'",
        "debug": "node %NODE_DEBUG_OPTION% ./src/app.js",
        "build:docker": "docker build -f docker/Dockerfile . -t vue-cloudfront-rest-api",
        "update:geoip-lite": "cd node_modules/geoip-lite && npm run-script updatedb",
        "start:ws": "node ./src/app.js",
        "start": "pm2 start ecosystem.json --no-daemon",
        "stop": "pm2 stop ecosystem.json",
        "lint": "eslint ./src"
    },
    "dependencies": {
        "bcrypt": "^3.0.5",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "geoip-lite": "^1.3.7",
        "graceful-fs": "^4.1.15",
        "i18n-iso-countries": "^3.7.8",
        "jsonschema": "^1.2.4",
        "jszip": "^3.2.1",
        "mongodb": "^3.2.2",
        "mongoose": "^5.4.22",
        "multer": "^1.4.1",
        "pm2": "^3.4.1",
        "ua-parser-js": "^0.7.19",
        "ws": "^6.2.1"
    },
    "devDependencies": {
        "eslint": "^5.16.0",
        "nodemon": "^1.18.10"
    }
}

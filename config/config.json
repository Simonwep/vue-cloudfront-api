{
    "dbname": "vue-cloudfront",
    "storagePath": "/_storage",
    "maxRequestSize": 5000000000,
    "streamChunkSize": 4096000,
    "userStorageLimit": -1,
    "disableRegistration": false,
    "defaultUIDLength": 8,
    "staticLinkUIDLength": 20,
    "defaultFolderColor": "#333333",
    "server": {
        "port": 8080
    },
    "auth": {
        "apikeyExpiry": 12960000000,
        "saltRounds": 10
    },
    "validation": {
        "username": "^[\\w\\d]{2,15}$",
        "password": "^(.){4,20}$",
        "dirname": "^(.){1,100}$",
        "hexcolor": "^#([\\dA-Fa-f]{6})$"
    }
}

{
    "dbname": "vue-cloudfront",
    "storagepath": "/_storage",
    "maxRequestSize": 5000000000,
    "disableRegistration": false,
    "server": {
        "port": 8080
    },
    "auth": {
        "apikeyExpiry": 12960000000,
        "saltRounds": 10
    },
    "validation": {
        "username": "^[\\w\\d]{2,15}$",
        "password": "^(.){4,20}$",
        "dirname": "^(.){1,100}$",
        "hexcolor": "^#([\\dA-Fa-f]{6})$"
    }
}

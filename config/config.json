{
    "dbname": "vue-cloudfront",
    "storagepath": "/data",
    "server": {
        "port": 4001
    },
    "auth": {
        "apikeyExpiry": 12960000000,
        "saltRounds": 10
    },
    "errors": {
        "invalid": {
            "username": "A username can contain only numbers, letters, and underscores.",
            "password": "Password need to have between 6 and 20 characters.",
            "apikey": "Invalid APIKey, please try logging out and back in."
        },
        "user": {
            "notFound": "User not found.",
            "alreadyExists": "Username already exists.",
            "wrongPassword": "Incorrect password."
        }
    },
    "validation": {
        "username": "^[\\w\\d]{2,15}$",
        "password": "^(.){4,20}$"
    }
}
